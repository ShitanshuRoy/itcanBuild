{
  "name": "express-api-boilerplate",
  "version": "0.0.0",
  "description": "Boilerplate for writing pretty Express-based APIs",
  "scripts": {
    "dev-server": "nodemon --exec \"babel-node --inspect src\"",
    "build": "babel -d ./build src",
    "start": "nodemon --exec \"NODE_ENV=production babel-node build/index.js\"",
    "test": "jest",
    "lint": "eslint 'src/**/*.js'",
    "fix-lint": "prettier --write './src/**/*.js' && npm run-script lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "express": "^4.16.1",
    "express-force-ssl": "^0.3.2",
    "express-request-language": "^1.1.12",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.0.1",
    "mongoose": "^5.6.8",
    "node-fetch": "^1.7.3",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.3"
  }
}
